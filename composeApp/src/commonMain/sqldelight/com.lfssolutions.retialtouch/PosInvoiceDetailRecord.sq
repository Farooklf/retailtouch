CREATE TABLE PosInvoiceDetailRecord (
     id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
     posInvoiceDetails TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO PosInvoiceDetailRecord(id,posInvoiceDetails)
VALUES(?,?);

getAll:
SELECT * FROM PosInvoiceDetailRecord;

delete:
DELETE  FROM PosInvoiceDetailRecord;

getCount:
SELECT COUNT(*) FROM PosInvoiceDetailRecord;