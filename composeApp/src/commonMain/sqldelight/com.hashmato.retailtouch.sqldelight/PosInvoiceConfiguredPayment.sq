CREATE TABLE PosInvoiceConfiguredPayment (
     id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
     posInvoiceConfiguredPayment TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO PosInvoiceConfiguredPayment(id,posInvoiceConfiguredPayment)
VALUES(?,?);

getAll:
